<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.gonnaup.accountmanagement.dao.AccountTokenDao">

    <resultMap type="org.gonnaup.accountmanagement.entity.AccountToken" id="AccountTokenMap">
        <result property="id" column="id" jdbcType="INTEGER"/>
        <result property="accountId" column="account_id" jdbcType="INTEGER"/>
        <result property="accountToken" column="account_token" jdbcType="VARCHAR"/>
        <result property="createtime" column="createtime" jdbcType="TIMESTAMP"/>
    </resultMap>

    <!--查询单个-->
    <select id="queryById" resultMap="AccountTokenMap">
        select
          id, account_id, account_token, createtime
        from acountManagement.account_token
        where id = #{id}
    </select>

    <!--查询指定行数据-->
    <select id="queryAllByLimit" resultMap="AccountTokenMap">
        select
          id, account_id, account_token, createtime
        from acountManagement.account_token
        limit #{offset}, #{limit}
    </select>

    <!--通过实体作为筛选条件查询-->
    <select id="queryAll" resultMap="AccountTokenMap">
        select
        id, account_id, account_token, createtime
        from acountManagement.account_token
        <where>
            <if test="id != null">
                and id = #{id}
            </if>
            <if test="accountId != null">
                and account_id = #{accountId}
            </if>
            <if test="accountToken != null and accountToken != ''">
                and account_token = #{accountToken}
            </if>
            <if test="createtime != null">
                and createtime = #{createtime}
            </if>
        </where>
    </select>

    <!--新增所有列-->
    <insert id="insert" keyProperty="id" useGeneratedKeys="true">
        insert into acountManagement.account_token(account_id, account_token, createtime)
        values (#{accountId}, #{accountToken}, #{createtime})
    </insert>

    <insert id="insertBatch" keyProperty="id" useGeneratedKeys="true">
        insert into acountManagement.account_token(account_id, account_token, createtime)
        values
        <foreach collection="entities" item="entity" separator=",">
            (#{entity.accountId}, #{entity.accountToken}, #{entity.createtime})
        </foreach>
    </insert>

    <insert id="insertOrUpdateBatch" keyProperty="id" useGeneratedKeys="true">
        insert into acountManagement.account_token(account_id, account_token, createtime)
        values
        <foreach collection="entities" item="entity" separator=",">
            (#{entity.accountId}, #{entity.accountToken}, #{entity.createtime})
        </foreach>
        on duplicate key update
        account_id = values(account_id) , account_token = values(account_token) , createtime = values(createtime)
    </insert>

    <!--通过主键修改数据-->
    <update id="update">
        update acountManagement.account_token
        <set>
            <if test="accountId != null">
                account_id = #{accountId},
            </if>
            <if test="accountToken != null and accountToken != ''">
                account_token = #{accountToken},
            </if>
            <if test="createtime != null">
                createtime = #{createtime},
            </if>
        </set>
        where id = #{id}
    </update>

    <!--通过主键删除-->
    <delete id="deleteById">
        delete from acountManagement.account_token where id = #{id}
    </delete>

</mapper>